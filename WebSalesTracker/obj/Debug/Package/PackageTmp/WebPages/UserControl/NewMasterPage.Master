                    return "ALABAMA";

                case 'AK':
                    return "ALASKA";

                case 'AS':
                    return "AMERICAN SAMOA";

                case 'AZ':
                    return "ARIZONA";

                case 'AR':
                    return "ARKANSAS";

                case 'CA':
