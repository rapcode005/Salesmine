                <div id="loading" style="position: fixed; width: 100%; text-align: center; top: 300px;">
                    <asp:Image ID="Image1" runat="server" ImageUrl="~/App_Themes/Images/indicator_medium.gif" />
                    <br />
                    Please wait...
